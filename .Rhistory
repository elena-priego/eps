"---",
paste0("title: ", experiment_name),
paste0("date: ", Sys.Date()),
"---",
"\n",
paste0("# ", experiment_name),
"\n",
"## Aim",
"\n",
"## Method",
"\n",
"## Results",
"\n",
paste0("path_builder(", experiment_name, "),"),
"\n",
"## Conclusions",
sep = "\n"
)
write(yalm,
file = here::here("doc", paste0(experiment_name, ".Rmd")),
append = TRUE)
}
